<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
    	<servlet-name>CartServlet</servlet-name>
    	<display-name>CartServlet</display-name>
    	<description></description>
    	<servlet-class>edu.mum.cs.cs472.controller.CartServlet</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>CheckoutServlet</servlet-name>
    	<display-name>CheckoutServlet</display-name>
    	<description></description>
    	<servlet-class>edu.mum.cs.cs472.controller.CheckoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>CartServlet</servlet-name>
    	<url-pattern>/CartServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
    	<servlet-name>CheckoutServlet</servlet-name>
    	<url-pattern>/CheckoutServlet</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
